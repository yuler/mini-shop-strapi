/* layer: default */
.invisible{visibility:hidden;}
.fixed{position:fixed;}
.bottom-5{bottom:40rpx;}
.left-0{left:0;}
.grid{display:grid;}
.grid-cols-2{grid-template-columns:repeat(2,minmax(0,1fr));}
.m-auto{margin:auto;}
.mb-2{margin-bottom:16rpx;}
.mt-2{margin-top:16rpx;}
.mt-4{margin-top:32rpx;}
.block{display:block;}
.h-18{height:144rpx;}
.h-20{height:160rpx;}
.h-25{height:200rpx;}
.h-480rpx{height:480rpx;}
.h-8{height:64rpx;}
.min-h-screen{min-height:100vh;}
.w-10{width:80rpx;}
.w-20{width:160rpx;}
.w-25{width:200rpx;}
.w-8{width:64rpx;}
.w-full{width:100%;}
.flex{display:flex;}
.flex-1{flex:1 1 0%;}
.flex-col{flex-direction:column;}
.items-center{align-items:center;}
.justify-between{justify-content:space-between;}
.gap-2{grid-gap:16rpx;gap:16rpx;}
.gap-4{grid-gap:32rpx;gap:32rpx;}
.overflow-hidden{overflow:hidden;}
.border{border-width:1px;}
.border-black{--un-border-opacity:1;border-color:rgba(0,0,0,var(--un-border-opacity));}
.rounded{border-radius:8rpx;}
.border-solid{border-style:solid;}
.bg-gray-200{--un-bg-opacity:1;background-color:rgba(229,231,235,var(--un-bg-opacity));}
.bg-white{--un-bg-opacity:1;background-color:rgba(255,255,255,var(--un-bg-opacity));}
.px-2{padding-left:16rpx;padding-right:16rpx;}
.px-4{padding-left:32rpx;padding-right:32rpx;}
.py-2{padding-top:16rpx;padding-bottom:16rpx;}
.py-3{padding-top:24rpx;padding-bottom:24rpx;}
.pb-10{padding-bottom:80rpx;}
.pb-2{padding-bottom:16rpx;}
.pb-20{padding-bottom:160rpx;}
.pt-4{padding-top:32rpx;}
.text-center{text-align:center;}
.text-2xl{font-size:48rpx;line-height:64rpx;}
.text-lg{font-size:36rpx;line-height:56rpx;}
.text-sm{font-size:28rpx;line-height:40rpx;}
.text-xl{font-size:40rpx;line-height:56rpx;}
.font-bold{font-weight:700;}
.leading-7{line-height:56rpx;}
.leading-8{line-height:64rpx;}
.text-gray-500{--un-text-opacity:1;color:rgba(107,114,128,var(--un-text-opacity));}
.text-gray-800{--un-text-opacity:1;color:rgba(31,41,55,var(--un-text-opacity));}